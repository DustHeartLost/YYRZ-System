<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <androidx.cardview.widget.CardView
        android:layout_width="0dp"
        android:layout_height="309dp"
        android:layout_margin="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_begin="51dp" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.85" />

            <TextView
                android:id="@+id/my_patient_textView1"
                android:layout_width="239dp"
                android:layout_height="29dp"
                android:fontFamily="sans-serif-smallcaps"
                android:textAllCaps="true"
                android:textColor="#009688"
                android:textSize="18sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toTopOf="@+id/my_patient_textView2"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/guideline2"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.25" />

            <TextView
                android:id="@+id/my_patient_textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#000000"
                android:textStyle="bold"
                app:layout_constraintBottom_toTopOf="@+id/my_patient_textView3"
                app:layout_constraintStart_toStartOf="@+id/my_patient_textView1"
                app:layout_constraintTop_toBottomOf="@+id/my_patient_textView1" />

            <TextView
                android:id="@+id/my_patient_textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#000000"
                android:textStyle="bold"
                app:layout_constraintBottom_toTopOf="@+id/my_patient_textView4"
                app:layout_constraintStart_toStartOf="@+id/my_patient_textView2"
                app:layout_constraintTop_toBottomOf="@+id/my_patient_textView2" />

            <TextView
                android:id="@+id/my_patient_textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#000000"
                android:textStyle="bold"
                app:layout_constraintBottom_toTopOf="@+id/my_patient_textView5"
                app:layout_constraintStart_toStartOf="@+id/my_patient_textView3"
                app:layout_constraintTop_toBottomOf="@+id/my_patient_textView3" />

            <TextView
                android:id="@+id/my_patient_textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#000000"
                android:textStyle="bold"
                app:layout_constraintBottom_toTopOf="@+id/my_patient_textView6"
                app:layout_constraintStart_toStartOf="@+id/my_patient_textView4"
                app:layout_constraintTop_toBottomOf="@+id/my_patient_textView4" />

            <TextView
                android:id="@+id/my_patient_textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#000000"
                android:textStyle="bold"
                app:layout_constraintBottom_toTopOf="@+id/guideline"
                app:layout_constraintStart_toStartOf="@+id/my_patient_textView5"
                app:layout_constraintTop_toBottomOf="@+id/my_patient_textView5" />

            <TextView
                android:id="@+id/my_patient_textView7"
                android:layout_width="59dp"
                android:layout_height="22dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/my_patient_textView2"
                app:layout_constraintTop_toTopOf="@+id/my_patient_textView2" />

            <TextView
                android:id="@+id/my_patient_textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="@+id/my_patient_textView7"
                app:layout_constraintTop_toTopOf="@+id/my_patient_textView3" />

            <TextView
                android:id="@+id/my_patient_textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="@+id/my_patient_textView8"
                app:layout_constraintTop_toTopOf="@+id/my_patient_textView4" />

            <TextView
                android:id="@+id/my_patient_textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="@+id/my_patient_textView9"
                app:layout_constraintTop_toTopOf="@+id/my_patient_textView5" />

            <TextView
                android:id="@+id/my_patient_textView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="@+id/my_patient_textView10"
                app:layout_constraintTop_toTopOf="@+id/my_patient_textView6" />

            <Button
                android:id="@+id/viewpager_button2"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:background="?attr/selectableItemBackground"
                android:clickable="false"
                android:contextClickable="false"
                android:text="查看病例"
                app:layout_constraintBottom_toBottomOf="@+id/divider"
                app:layout_constraintEnd_toStartOf="@+id/my_patient_imageView"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="@+id/divider"
                app:layout_constraintTop_toTopOf="@+id/divider"
                app:layout_constraintVertical_bias="0.0" />

            <ImageView
                android:id="@+id/my_patient_imageView"
                android:layout_width="020dp"
                android:layout_height="0dp"
                android:background="#FFFFFF"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/divider"
                app:layout_constraintTop_toBottomOf="@+id/guideline"
                app:srcCompat="@drawable/right_black"
                tools:srcCompat="@drawable/right_black" />

            <View
                android:id="@+id/divider"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:background="?attr/selectableItemBackground"
                android:clickable="true"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/guideline" />

            <ImageButton
                android:id="@+id/imageButton2"
                android:layout_width="30dp"
                android:layout_height="29dp"
                android:background="?attr/selectableItemBackground"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/menu" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>