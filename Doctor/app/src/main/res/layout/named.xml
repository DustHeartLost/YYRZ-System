<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.yyrz.doctor.ui.assessmentUI.Named">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/named_imageView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="?attr/selectableItemBackground"
            android:clickable="true"
            android:src="@drawable/voice"
            app:layout_constraintBottom_toBottomOf="@+id/named_textView4"
            app:layout_constraintStart_toEndOf="@+id/named_textView4"
            app:layout_constraintTop_toTopOf="@+id/named_textView4" />

        <ImageView
            android:id="@+id/named_imageView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="?attr/selectableItemBackground"
            android:clickable="true"
            android:src="@drawable/voice"
            app:layout_constraintBottom_toBottomOf="@+id/named_textView3"
            app:layout_constraintStart_toEndOf="@+id/named_textView3"
            app:layout_constraintTop_toTopOf="@+id/named_textView3" />

        <ImageView
            android:id="@+id/named_imageView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="?attr/selectableItemBackground"
            android:clickable="true"
            android:src="@drawable/voice"
            app:layout_constraintBottom_toBottomOf="@+id/named_textView2"
            app:layout_constraintStart_toEndOf="@+id/named_textView2"
            app:layout_constraintTop_toTopOf="@+id/named_textView2" />

        <TextView
            android:id="@+id/named_textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/grade_hints"
            android:textAlignment="center"
            android:textColor="#000000"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/named_guideline3" />

        <TextView
            android:id="@+id/named_textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/named_label1"
            android:textAlignment="center"
            android:textColor="#000000"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/named_textView3"
            app:layout_constraintStart_toStartOf="@+id/guideline4"
            app:layout_constraintTop_toBottomOf="@+id/named_guideline3" />

        <TextView
            android:id="@+id/named_textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/named_label2"
            android:textAlignment="center"
            android:textColor="#000000"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/named_textView4"
            app:layout_constraintStart_toStartOf="@+id/named_textView2"
            app:layout_constraintTop_toBottomOf="@+id/named_textView2" />

        <TextView
            android:id="@+id/named_textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/named_label3"
            android:textAlignment="center"
            android:textColor="#000000"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/named_guideline1"
            app:layout_constraintStart_toStartOf="@+id/named_textView3"
            app:layout_constraintTop_toBottomOf="@+id/named_textView3" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/named_guideline1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.8" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/named_guideline3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.1" />

        <RadioGroup
            android:id="@+id/named_radioGroup1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:checkedButton="@id/named_radioButton1"
            android:orientation="horizontal"
            app:layout_constraintBottom_toBottomOf="@+id/named_textView2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/named_textView2"
            app:layout_constraintTop_toTopOf="@+id/named_textView2">

            <RadioButton
                android:id="@+id/named_radioButton1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/grade_0" />

            <Space
                android:layout_width="90dp"
                android:layout_height="32dp"
                android:layout_weight="1" />

            <RadioButton
                android:id="@+id/named_radioButton2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/grade_1" />

        </RadioGroup>

        <RadioGroup
            android:id="@+id/named_radioGroup2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:checkedButton="@id/named_radioButton3"
            android:orientation="horizontal"
            app:layout_constraintBottom_toBottomOf="@+id/named_textView3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/named_textView3"
            app:layout_constraintTop_toTopOf="@+id/named_textView3">

            <RadioButton
                android:id="@+id/named_radioButton3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/grade_0" />

            <Space
                android:layout_width="90dp"
                android:layout_height="32dp"
                android:layout_weight="1" />

            <RadioButton
                android:id="@+id/named_radioButton4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/grade_1" />

        </RadioGroup>

        <RadioGroup
            android:id="@+id/named_radioGroup3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:checkedButton="@id/named_radioButton5"
            android:orientation="horizontal"
            app:layout_constraintBottom_toBottomOf="@+id/named_textView4"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/named_textView4"
            app:layout_constraintTop_toTopOf="@+id/named_textView4">

            <RadioButton
                android:id="@+id/named_radioButton5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/grade_0" />

            <Space
                android:layout_width="90dp"
                android:layout_height="32dp"
                android:layout_weight="1" />

            <RadioButton
                android:id="@+id/named_radioButton6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/grade_1" />
        </RadioGroup>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.1" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>