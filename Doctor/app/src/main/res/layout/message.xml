<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.victor.loading.book.BookLoading
        android:id="@+id/bookLoading"
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.342">

    </com.victor.loading.book.BookLoading>

    <TextView
        android:id="@+id/assessment_finish_textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="加载中..."
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="@+id/bookLoading"
        app:layout_constraintStart_toStartOf="@+id/bookLoading"
        app:layout_constraintTop_toBottomOf="@+id/bookLoading" />


</androidx.constraintlayout.widget.ConstraintLayout>
